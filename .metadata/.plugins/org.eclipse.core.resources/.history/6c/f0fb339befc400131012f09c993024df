package com.rozar.random.codes;

public class NumberWithSeven {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// numbersWithSeven(800);
		numbers(1000);
	}
	
	public static void numbers(int number){
		for(int i = 7 ; i < number ; ){			
			int temp = i + 3;
			if(temp % 10 == 0){
				boolean result = false;
				int temp2 = temp;
				do{
					int tempR = temp2 % 10;
					int tempQ = temp2 / 10;
					if(tempR == 7){
						result = true;
					}
					temp2 = tempQ;
				}while(temp2 > 0);
				if(result){
					for(int j = 0 ; j < 10; j++){
						System.out.println(temp+" ");
						temp = temp + 1;
					}
					i = i + 10;
				}else
					i = i + 10;
			}
			else{
				i = i + 10;
			}
		}
	}
	
	public static void numbersWithSeven(int number){
		for(int i = 7 ; i <= number;){
			System.out.print(i + " ");
			int temp = i+3;
			if(i == 697){
				System.out.println();
			}
			if( temp % 10 == 0){
				boolean result = false;
				int temp2 = temp;
				do{
					int tempR = temp2 % 10;
					int tempQ = temp2 / 10;
					if(tempR == 7){
						result = true;
					}
					temp2 = tempQ;
				}while(temp2 > 0);
				if(result){
					for( ; i < i + 10; i++){
						System.out.print(i+ " ");
					}
					result = false;
					i = i+3+8+9;
				}		
				i = i+10;
			}else			
				i = i+10;			
		}
	}

}
