
public class SmallMultiple { 
    public static void main(String[] args) {
       int sml=0;
       outerloop:
        for(int i=40;i<100000;i++){
            int j=1;
            do{
            if(i%j==0)
                j++;                
            }while(j<21);
            if(j==20){
            sml=i;
            break outerloop;
            }   
            }
        System.out.println(sml);
    }
}