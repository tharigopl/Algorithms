	public static void numbersWithSeven(int number){
		
		/*for(int i = 1 ; i <= number; i++){
			if(number < 10 && number == 7){
				System.out.println(number);
				continue;
			}
			while(i/10 != 0){
				
			}
			
		}*/
		for(int i = 7 ; i <= number;){
			System.out.print(i + " ");
			int temp = i+3;
			if( temp % 7 == 0){
				for(int j = 0 ; j < 10; j++){
					System.out.println(i);
					i++;
				}
				i = i+8;
			}			
			i = i+10;			
		}
	}

}
